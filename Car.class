class ObjMass {
    static Car[] abc = new Car[3];

    public static void main(String[] args) {
        abc[0] = new Car("Tesla1");
        abc[1] = new Car("Tesla2");
        abc[2] = new Car("Tesla3");

        for (int i = 0; i < 3; i++) {
            System.out.println(abc[i].id);
        }
    }
}

public class Car {
    String id;

    public Car(String id) {
        this.id = id;
    }
}
